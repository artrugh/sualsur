{
  "name": "subiendoalsur",
  "version": "1.0.0",
  "description": "learning node, express and mongoDB",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass ./sass/main.scss ./public/css/style.css -w",
    "start": "node ./server.js",
    "start:dev": "nodemon ./server.js",
    "start:prod": "NODE_ENV=production nodemon ./server.js",
    "debug": "ndb server.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "compile:sass": "node-sass sass/main.scss ./public/css/style.comp.css",
    "concat:css": "concat -o ./public/css/style.concat.css  ./public/css/icon-font.css ./public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' ./public/css/style.concat.css -o ./public/css/style.prefix.css",
    "compress:css": "node-sass ./public/css/style.prefix.css ./public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "arturo",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "@sendgrid/mail": "^7.2.1",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.17",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.8",
    "pug": "^3.0.0",
    "sharp": "^0.25.4",
    "slugify": "^1.4.0",
    "stripe": "^8.63.0",
    "validator": "^13.0.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.4",
    "concat": "^1.0.3",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5"
  },
  " engines": {
    "node": "^10"
  }
}
